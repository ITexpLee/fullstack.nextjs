{
    "name": "nextjs-fullstackinator",
    "version": "1.0.0",
    "description": "Full of stack to the max. Nextjs with custom express server, Typescript, Redux, Saga, Styled-components and more",
    "private": true,
    "scripts": {
        "dev": "nodemon",
        "mongo": "mongod --auth --port 27017 --dbpath C:/database/mongo/db1",
        "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
        "build:client": "next build ./client",
        "build:server": "tsc --project tsconfig.server.json",
        "start": "cross-env NODE_ENV=production node dist/server"
    },
    "author": "Kyle Gallagher",
    "license": "MIT",
    "dependencies": {
        "@zeit/next-typescript": "^1.1.1",
        "es6-promise": "^4.2.5",
        "express": "^4.16.4",
        "express-graphql": "^0.7.1",
        "graphql": "^14.1.1",
        "mongodb": "^3.2.2",
        "mongoose": "^5.4.19",
        "next": "^8.0.3",
        "next-compose-plugins": "^2.1.1",
        "next-redux-saga": "^3.0.0",
        "next-redux-wrapper": "^2.0.0",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-redux": "^6.0.1",
        "redux": "^4.0.1",
        "redux-actions": "^2.6.5",
        "redux-saga": "^1.0.2",
        "styled-components": "^4.2.0"
    },
    "devDependencies": {
        "@types/express": "^4.16.1",
        "@types/express-graphql": "^0.6.2",
        "@types/graphql": "^14.0.7",
        "@types/mongodb": "^3.1.22",
        "@types/mongoose": "^5.3.23",
        "@types/next": "^8.0.1",
        "@types/node": "^11.11.4",
        "@types/react": "^16.8.8",
        "@types/react-dom": "^16.8.2",
        "babel-plugin-styled-components": "^1.10.0",
        "concurrently": "^4.1.0",
        "cross-env": "^5.2.0",
        "nodemon": "^1.18.10",
        "ts-node": "^8.0.3",
        "typescript": "^3.3.4000"
    }
}
