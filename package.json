{
    "name": "nextjs-fullstackinator",
    "version": "1.0.0",
    "description": "Full of stack to the max. Nextjs with custom express server, Typescript, Redux, Saga, Styled-components and more",
    "private": true,
    "scripts": {
        "start": "nodemon",
        "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
        "build:client": "next build ./client",
        "build:server": "tsc --project tsconfig.server.json",
        "production": "cross-env NODE_ENV=production node dist/server"
    },
    "author": "Kyle Gallagher",
    "license": "MIT",
    "dependencies": {
        "@zeit/next-typescript": "^1.1.1",
        "es6-promise": "^4.2.8",
        "express": "^4.17.1",
        "next": "^8.1.0",
        "next-compose-plugins": "^2.2.0",
        "next-redux-saga": "^3.0.0",
        "next-redux-wrapper": "^2.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "redux": "^4.0.1",
        "redux-actions": "^2.6.5",
        "redux-saga": "^1.0.4",
        "styled-components": "^4.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.0",
        "@types/express-graphql": "^0.8.0",
        "@types/graphql": "^14.2.2",
        "@types/mongodb": "^3.1.28",
        "@types/mongoose": "^5.5.6",
        "@types/next": "^8.0.5",
        "@types/node": "^12.0.10",
        "@types/react": "^16.8.22",
        "@types/react-dom": "^16.8.4",
        "babel-plugin-styled-components": "^1.10.4",
        "concurrently": "^4.1.1",
        "cross-env": "^5.2.0",
        "nodemon": "^1.19.1",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.2"
    }
}
